@article{ ISI:000593230900030,
Author = {Chen, Guoping and Ma, Suhui and Tian, Di and Xiao, Wen and Jiang, Lai
   and Xing, Aijun and Zou, Anlong and Zhou, Luhong and Shen, Haihua and
   Zheng, Chengyang and Ji, Chengjun and He, Hongbo and Zhu, Biao and Liu,
